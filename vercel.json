{
  "version": 2,
  "buildCommand": "cd Frontend && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "Frontend/build",
  "framework": null,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|json|txt|woff|woff2))",
      "dest": "Frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/build/index.html"
    }
  ]
}
